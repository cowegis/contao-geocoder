{
  "name": "cowegis/contao-geocoder",
  "type": "contao-bundle",
  "description": "Geocoder integration in Contao CMS",
  "keywords": [
    "geocoder",
    "api",
    "contao"
  ],
  "homepage": "https://cowegis.org",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "David Molineus",
      "email": "david@hofff.com",
      "homepage": "https://www.hofff.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "beberlei/assert": "^3.0",
    "contao-community-alliance/meta-palettes": "^2.0",
    "contao/core-bundle": "^4.4",
    "doctrine/cache": "^1.8",
    "geocoder-php/cache-provider": "^4.1",
    "geocoder-php/chain-provider": "^4.0",
    "geocoder-php/google-maps-provider": "^4.4",
    "geocoder-php/nominatim-provider": "^5.1",
    "netzmacht/contao-form-bundle": "^1.0",
    "netzmacht/contao-toolkit": "^3.3",
    "php-http/guzzle6-adapter": "^1.1 || ^2.0",
    "php-http/httplug": "^2.0",
    "psr/simple-cache": "^1.0",
    "roave/doctrine-simplecache": "^2.2",
    "symfony/config": "^3.3 || ^4.0",
    "symfony/dependency-injection": "^3.3 || ^4.0",
    "symfony/form": "^3.4 || ^4.0",
    "symfony/http-foundation": "^3.4 || ^4.0",
    "symfony/http-kernel": "^3.3 || ^4.0",
    "symfony/routing": "^3.4 || ^4.0",
    "symfony/twig-bundle": "^3.4 || ^4.0",
    "willdurand/geocoder": "^4.2"
  },
  "conflict": {
    "contao/manager-plugin": "<2.1 || >= 3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "doctrine/coding-standard": "^5.0",
    "pdepend/pdepend": "^2.5",
    "phpcq/autoload-validation": "^1.2",
    "phpcq/branch-alias-validation": "^1.2",
    "phpcq/phpcq": "^1.2",
    "phploc/phploc": "^4.0 || ^5.0",
    "phpmd/phpmd": "^2.6",
    "phpspec/phpspec": "^5.1",
    "phpstan/phpstan": "^0.11.8",
    "sebastian/phpcpd": "^4.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev",
      "dev-develop": "0.2.x-dev"
    },
    "contao-manager-plugin": "Cowegis\\ContaoGeocoder\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Cowegis\\ContaoGeocoder\\": "src/"
    }
  }
}
